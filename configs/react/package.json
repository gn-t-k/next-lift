{
    "name": "@configs/react",
    "private": true,
    "type": "module",
    "exports": {
        "./tsconfig/tsconfig.json": "./src/tsconfig/tsconfig.json",
        "./vite": {
            "types": "./src/vite/vite.config.ts",
            "import": "./dist/vite/vite.config.mjs"
        },
        "./eslint": {
            "types": "./src/eslint/eslint.config.ts",
            "import": "./dist/eslint/eslint.config.mjs"
