        "@tsconfig/create-react-app": "2.0.5",
        "@vitejs/plugin-react": "4.5.2",
        "eslint-plugin-storybook": "9.0.10",
        "jsdom": "26.1.0",
        "vite": "6.3.5",
        "vitest": "3.2.3"
    },
    "devDependencies": {
        "eslint": "9.29.0",
        "pkgroll": "2.13.1",
        "typescript": "5.8.3"
    }
